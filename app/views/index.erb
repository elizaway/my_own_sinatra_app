<h2>Here are all the jewels that are inside the box:</h2>
<h3>Jewels:</h3>
<ul>
<% @jewels.each do |jewel| %>
<li>
  <a href='/jewels/<%= jewel.id %>'>
    <%=jewel.mineral%>
  </a>
    <div>
      <a href='/jewels/<%= jewel.id %>/edit'>
        edit
      </a>
      <form action="/jewels/<%= jewel.id %>" method="post">
        <input type="hidden" name="_method" value="delete">
        <input type="submit" value="delete">
      </form>
    </div>
</li>
<% end %>
</ul>
<a href="/jewels/new">Click Here to Make a new Jewel!</a>
